{
  "name": "frontend",
  "version": "1.0.0",
  "main": " ",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "kesaseteli/*",
    "benefit/*",
    "tet/*",
    "shared"
  ],
  "scripts": {
    "audit": "npx lerna-audit --level=high --groups=dependencies",
    "ks-empl:build": "npx lerna run --scope @frontend/employer --stream build",
    "ks-handler:build": "npx lerna run --scope @frontend/ks-handler --stream build",
    "ks-youth:build": "npx lerna run --scope @frontend/youth --stream build",
    "bf-applicant:build": "npx lerna run --scope @frontend/applicant --stream build",
    "bf-handler:build": "npx lerna run --scope @frontend/bf-handler --stream build",
    "ks-empl:dev": "npx lerna run --scope @frontend/employer --stream dev",
    "ks-handler:dev": "npx lerna run --scope @frontend/ks-handler --stream dev",
    "ks-youth:dev": "npx lerna run --scope @frontend/youth --stream dev",
    "bf-applicant:dev": "npx lerna run --scope @frontend/applicant --stream dev",
    "bf-handler:dev": "npx lerna run --scope @frontend/bf-handler --stream dev",
    "ks-empl:start": "npx lerna run --scope  @frontend/employer --stream start",
    "ks-handler:start": "npx lerna run --scope  @frontend/ks-handler --stream start",
    "ks-youth:start": "npx lerna run --scope  @frontend/youth --stream start",
    "bf-applicant:start": "npx lerna run --scope  @frontend/applicant --stream start",
    "bf-handler:start": "npx lerna run --scope  @frontend/bf-handler --stream start",
    "release": "cross-env HUSKY_BYPASS=true && lerna publish",
    "husky:pre-commit": "npx lerna run --concurrency 1 --stream pre-commit",
    "lint": "npx lerna run lint",
    "test": "npx lerna run test --stream",
    "test:debug": "npx lerna run test:debug",
    "test:staged": "npx lerna run test:staged",
    "test:coverage": "npx lerna run test:coverage --stream --concurrency 1",
    "bf-applicant:test": "yarn --cwd benefit/applicant test",
    "bf-handler:test": "yarn --cwd benefit/bf-handler test",
    "bf-applicant:browser-test": "yarn --cwd benefit/applicant browser-test",
    "bf-applicant:browser-test:ci": "yarn --cwd benefit/applicant browser-test:ci",
    "ks-empl:test": " yarn --cwd kesaseteli/employer test",
    "ks-empl:browser-test": "yarn --cwd kesaseteli/employer browser-test",
    "ks-empl:browser-test:ci": "yarn --cwd kesaseteli/employer browser-test:ci",
    "ks-youth:test": " yarn --cwd kesaseteli/youth test",
    "ks-youth:browser-test": "yarn --cwd kesaseteli/youth browser-test",
    "ks-youth:browser-test:ci": "yarn --cwd kesaseteli/youth browser-test:ci",
    "shared:test": " yarn --cwd shared test",
    "ks-shared:test": " yarn --cwd kesaseteli/shared test",
    "te-empl:build": "npx lerna run --scope @frontend/te-empl --stream build",
    "te-empl:dev": "npx lerna run --scope @frontend/te-empl --stream dev",
    "te-empl:start": "npx lerna run --scope  @frontend/te-empl --stream start",
    "te-empl:test": " yarn --cwd tet/employer test",
    "te-empl:browser-test": "yarn --cwd tet/employer browser-test",
    "te-empl:browser-test:ci": "yarn --cwd tet/employer browser-test:ci",
    "te-admn:build": "npx lerna run --scope @frontend/te-admn --stream build",
    "te-admn:dev": "npx lerna run --scope @frontend/te-admn --stream dev",
    "te-admn:start": "npx lerna run --scope  @frontend/te-admn --stream start",
    "te-admn:test": " yarn --cwd tet/admin test",
    "te-admn:browser-test": "yarn --cwd tet/admin browser-test",
    "te-admn:browser-test:ci": "yarn --cwd tet/admin browser-test:ci",
    "te-yout:build": "npx lerna run --scope @frontend/te-yout --stream build",
    "te-yout:dev": "npx lerna run --scope @frontend/te-yout --stream dev",
    "te-yout:start": "npx lerna run --scope  @frontend/te-yout --stream start",
    "te-yout:test": " yarn --cwd tet/youth test",
    "te-yout:browser-test": "yarn --cwd tet/youth browser-test",
    "te-yout:browser-test:ci": "yarn --cwd tet/youth browser-test:ci"
  },
  "dependencies": {},
  "devDependencies": {
    "@axe-core/react": "^4.3.2",
    "@babel/core": "^7.16.7",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^16.0.0",
